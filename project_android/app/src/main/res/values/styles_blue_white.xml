<?xml version="1.0" encoding="utf-8"?><resources>    <style name="Button.Primary.Round" parent="Widget.AppCompat.Button.Small">        <item name="android:background">@drawable/corner_button_bg_drawable</item>        <item name="android:textColor">?attr/buttonTextColor</item>        <item name="android:textAllCaps">false</item>        <item name="android:textSize">@dimen/s13</item>    </style>    <style name="Gray.Button.Primary.Round" parent="Widget.AppCompat.Button.Small">        <item name="android:background">@drawable/corner_button_secondary_drawable</item>        <item name="android:textColor">?attr/buttonTextColor</item>        <item name="android:textAllCaps">false</item>        <item name="android:textSize">@dimen/s13</item>    </style>    <style name="Blue.Button.Primary.Dark.Round" parent="Widget.AppCompat.Button.Small">        <item name="android:background">@drawable/primary_dark_corner_button_bg_drawable</item>        <item name="android:textColor">?attr/buttonTextColor</item>        <item name="android:textAllCaps">false</item>        <item name="colorAccent">?attr/colorPrimaryDark</item>    </style>    <style name="Light.Gray.Primary.Round" parent="Widget.AppCompat.Button.Small">        <item name="android:background">@drawable/primary_light_gray_corner_button_bg_drawable</item>        <item name="android:textColor">?attr/buttonTextColor</item>        <item name="android:textAllCaps">false</item>        <item name="colorAccent">?attr/colorPrimaryDark</item>    </style>    <style name="EdiText.Primary.HintTextStyle" parent="@android:style/TextAppearance">        <item name="colorControlNormal">?attr/secondaryTextColor</item>        <item name="colorControlActivated">?attr/secondaryTextColor</item>        <item name="colorControlHighlight">?attr/secondaryTextColor</item>        <item name="android:textSize">@dimen/s14</item>        <item name="android:textColor">?attr/primaryTextColor</item>        <item name="android:textColorHint">?attr/secondaryTextColor</item>        <item name="android:maxLength">35</item>        <item name="android:fontFamily">@font/lato_bold</item>        <item name="android:background">@null</item>        <item name="android:drawableBottom">@null</item>    </style></resources>